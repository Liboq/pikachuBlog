{
  "editor.codeActionsOnSave": {
      "source.fixAll": true,
      "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
      "vue",
      "javascript",
      "javascriptreact",
      "html",
  ],
  "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
   "eslint.options": {
     "overrideConfig": {
        "env": {
        "browser": true,
        "es6": true
         },
      "parserOptions": {
         "ecmaVersion": 2019,
          "sourceType": "module",
          "ecmaFeatures": {
                    "jsx": true
                }
    },
    "rules": {
      "no-debugger": "off"
    }
  }
},
}